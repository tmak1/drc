  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "type": "pwa-chrome",
      "request": "attach",
      "urlFilter": "https://localhost/*",
      "webRoot": "/home/node/",
      "sourceMapPathOverrides": {
        "/*": "/__vscode-remote-uri__/*"
      }
    }
  ]
}
